<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <TargetFramework>net7.0-windows10.0.22621.0</TargetFramework>
    <ImplicitUsings>disable</ImplicitUsings>
    <Nullable>disable</Nullable>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <UseWindowsForms>true</UseWindowsForms>
    <ApplicationManifest>app.manifest</ApplicationManifest>
    <SupportedOSPlatformVersion>7.0</SupportedOSPlatformVersion>
    <StartupObject>MysteryMemeware.Program</StartupObject>
    <AssemblyName>MysteryMemeware</AssemblyName>
    <RootNamespace>MysteryMemeware</RootNamespace>
    <ApplicationIcon>Icon.ico</ApplicationIcon>
    <ProduceReferenceAssembly>False</ProduceReferenceAssembly>
    <ErrorReport>none</ErrorReport>
    <PackageId>MysteryMemeware</PackageId>
    <Title>MysteryMemeware</Title>
    <Version>1.0.0.0</Version>
    <Authors>RandomiaGaming</Authors>
    <Company>RandomiaGaming</Company>
    <Product>MysteryMemeware</Product>
    <Description>Will you ever be able to use your pc again? It's a mystery! Depends upon MysteryHelper.</Description>
    <Copyright>Copyright Â© RandomiaGaming 2022</Copyright>
    <PackageProjectUrl>https://github.com/RandomiaGaming/MysteryMemeware</PackageProjectUrl>
    <PackageIcon>Icon.png</PackageIcon>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <RepositoryUrl>https://github.com/RandomiaGaming/MysteryMemeware</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>virus,malware,memeware</PackageTags>
    <NeutralLanguage>en</NeutralLanguage>
    <BaseOutputPath>D:\Pentesting Portal</BaseOutputPath>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants />
    <DebugType>full</DebugType>
    <NoWarn />
    <WarningsAsErrors />
    <CheckForOverflowUnderflow>True</CheckForOverflowUnderflow>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DefineConstants />
    <DebugType>full</DebugType>
    <NoWarn />
    <WarningsAsErrors />
    <CheckForOverflowUnderflow>True</CheckForOverflowUnderflow>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="BackgroundSong.wav" />
    <None Remove="CoverImage.bmp" />
    <None Remove="CoverImageWithFailSafe.bmp" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="Icon.ico" />
  </ItemGroup>
  <ItemGroup>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\MysteryHelpers\MysteryHelper.csproj" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="BackgroundSong.wav" />
    <EmbeddedResource Include="CoverImage.bmp" />
    <EmbeddedResource Include="CoverImageWithFailSafe.bmp" />
  </ItemGroup>
  <ItemGroup>
    <None Update="Icon.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>
</Project>
